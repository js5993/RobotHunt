<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.junhosung.robothunt">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".views.activities.GameActivity" />
        <activity android:name=".views.activities.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.activities.MainMenuActivity" />
        <activity android:name=".views.activities.OptionActivity" />
        <activity
            android:name=".views.activities.HelpActivity"
            android:label="@string/title_activity_help" />
        <activity android:name=".views.activities.ScoreboardActivity"></activity>
    </application>

</manifest>